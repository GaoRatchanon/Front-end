<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Types</title>
</head>
<body>
    <h1>Types</h1>
    <table>
        <tr>
            <th>Type ID</th>
            <th>Category</th>
            <th>Total Task</th>
            <th>Action</th>
        </tr>
        <% for(var i=0; i<types.length; i++) { %>
        <tr>
            <td><%= types[i].type_id %></td>
            <td><%= types[i].category %></td>
            <td><%= types[i].totalTask %></td>
            <td>
                <a href="/taskontype/<%= types[i].type_id %>">View</a>
                <a href="/updateTypes/<%= types[i].type_id %>">Edit</a>
                <a href="/deleteTypes/<%= types[i].type_id %>">Delete</a>
            </td>
        </tr>    
        <%} %>
    </table>
    <a href="/createTypes">createType</a>
    <a href="/">Go to Task</a>
</body>
</html>